{
  "name": "bingx-web-helper",
	"displayName": "bingx-web-helper",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.67.0"
	},
  "repository": "https://github.com/QingshanLuoyue/bingx-web-helper",
  "publisher": "linjunfu",
	"categories": [
		"Other"
	],
	"activationEvents": [
    "*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "bingx-web-helper.helloWorld",
				"title": "bingx Hello World"
			},
      {
        "command": "showImage",
        "icon": {
            "light": "./src/assets/img/icon-img-light.svg",
            "dark": "./src/assets/img/icon-img-dark.svg"
        },
        "title": "showImage"
      }
		],
    "menus": {
      "editor/title": [{
          "when": "resourceLangId == vue",
          "command": "showImage",
          "group": "navigation"
      }]
    }
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"ts-loader": "^9.2.8",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"@vscode/test-electron": "^2.1.3",
    "@types/glob": "7.1.1",
		"@types/mocha": "7.0.2",
		"@types/node": "13.11.0",
		"@types/vscode": "1.46.0",
		"eslint": "6.8.0",
		"glob": "7.1.6",
		"mocha": "7.1.2",
		"typescript": "3.8.3",
		"vscode-test": "1.3.0"
	},
  "dependencies": {
		"@babel/core": "7.11.1",
		"@babel/generator": "7.11.0",
		"@babel/parser": "7.11.2",
		"@babel/plugin-proposal-class-properties": "7.10.4",
		"@babel/plugin-proposal-decorators": "7.10.5",
		"@babel/preset-env": "7.11.0",
		"@babel/preset-typescript": "7.10.4",
		"@babel/traverse": "7.11.0",
		"@babel/types": "7.11.0",
		"escodegen": "2.0.0",
		"esprima": "4.0.1",
		"estraverse": "5.2.0"
	}
}
